algoritmo "Teste vetor"
var
  valor: vetor[1..10] de inteiro
  conta: inteiro
inicio
   Para conta<-1 ate 10 faca
       escreva("Digite o ",conta, "o. valor: ")
       leia(valor[conta])
   FimPara
   
   
   Para conta <-1 ate 10 faca
      escreva ("{",valor[conta], "}")
   FimPara
fimalgoritmo